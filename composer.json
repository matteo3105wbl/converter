{
    "name": "matteo3105wbl/converter",
    "description": "Custom converter with different features",
    "keywords": ["converter", "matteo3105wbl", "console", "cli"],
    "homepage": "https://laravel-zero.com",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Matteo Weble",
            "email": "matteo@weble.it"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel-zero/framework": "^10.2",
        "laravel-zero/phar-updater": "^1.3",
        "nunomaduro/termwind": "^1.15.1",
        "php-ffmpeg/php-ffmpeg": "^1.2"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^2.22"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["converter"],
    "repositories": [{
        "type": "github",
        "url": "https://github.com/akeeba/remotecli"
    }]
}
